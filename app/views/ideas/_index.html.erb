<table class="ideas-table table">
    <!--Table head-->
    <thead class="">
        <tr>
            <th>ID</th>
            <th>User</th>
            <th>Name</th>
            <th colspan="2">Actions</th>
        </tr>
    </thead>
    <!--Table head-->
    <!--Table body-->
    <tbody class="table-dark" id="ideas-true">
        <% ideas.each do |idea| %>
        <%= render 'ideas/on_idea', idea: idea %>
        <% end %>
    </tbody>
    <!--Table body-->
</table>
<div class="w-100">
    <div class="mx-sm-0 mx-md-auto">
        <%== pagy_bootstrap_nav(pagy) %>
    </div>
    <%#= pagy_bootstrap_nav_js(pagy) %>
    <%#= pagy_bootstrap_combo_nav_js(pagy) %>
    <%#= render partial: 'pagy/nav', locals: {pagy: pagy} %>
</div>
<!--Table-->